{
  "private": true,
  "displayName": "HS Plug",
  "name": "homebridge-hs-plug",
  "version": "1.0.0",
  "description": "Provides the ability to run TP-Link HS1XX plugs with HW:4.1 and SW:1.1.0",
  "license": "Apache-2.0",
  "repository": {
